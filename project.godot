; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "ArrayUtils",
"language": "GDScript",
"path": "res://addons/SIsilicon.3d.text/array_utils.gd"
}, {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
}, {
"base": "ResourceFormatLoader",
"class": "HTerrainDataLoader",
"language": "GDScript",
"path": "res://addons/zylann.hterrain/hterrain_resource_loader.gd"
}, {
"base": "ResourceFormatSaver",
"class": "HTerrainDataSaver",
"language": "GDScript",
"path": "res://addons/zylann.hterrain/hterrain_resource_saver.gd"
} ]
_global_script_class_icons={
"ArrayUtils": "",
"GitAPI": "",
"HTerrainDataLoader": "",
"HTerrainDataSaver": ""
}

[application]

config/name="Economics"
config/description="Do everything everywhere"
run/main_scene="res://utils/loader/Loader.tscn"
boot_splash/image="res://logo.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.105882, 0.105882, 0.105882, 1 )
config/icon="res://logo.png"

[audio]

enable_audio_input=true

[autoload]

UserSettings="*res://utils/client/Settings/UserSettings.cs"
BackgroundLoader="*res://utils/loader/BackgroundLoader.cs"
HTerrainThreadFix="*res://fixes/HTerrainThreadFix.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false

[editor]

search_in_file_extensions=PoolStringArray( "gd", "shader", "cs", "pose", "reciepe" )

[editor_plugins]

enabled=PoolStringArray( "SIsilicon.3d.text", "footsteps", "mesh_lod", "uma", "zylann.hterrain" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

[global]

Debug=true

